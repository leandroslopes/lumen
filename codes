function is_email(email){
	er = /^[a-zA-Z0-9][a-zA-Z0-9\._-]+@([a-zA-Z0-9\._-]+\.)[a-zA-Z-0-9]{2,3}/; 
	if( !er.exec(email) )
	{
		jQuery('#retorno_capta').html('Email inv√°lido!');
		return false;
	}
}


	
